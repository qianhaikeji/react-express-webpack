{
  "name": "express-react-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:client:prod || npm run start:client:dev",
    "start:client:dev": "webpack-dev-server --config webpack/webpack.config.js --inline --content-base client/ --port 9000 --history-api-fallback",
    "start:client:prod": "npm run build:client && nodemon server.js",
    "start:server": "npm run build:server && nodemon server.bundle.js",
    "clean:client": "rimraf public",
    "clean:server": "rimraf server.bundle.js",
    "build:all": "npm run build:client && npm run build:server",
    "build:client": "npm run clean:client && webpack --config webpack/webpack.config.js",
    "build:server": "npm run clean:server && webpack --config webpack/webpack.server.config.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "if-env": "^1.0.0",
    "react-parallax": "^0.4.3"
  },
  "devDependencies": {
    "antd": "^1.6.1",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-antd": "^0.4.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs": "^0.4.0",
    "html-webpack-plugin": "^2.16.1",
    "http-server": "^0.8.5",
    "immutable": "^3.8.1",
    "nodemon": "^1.9.2",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-thunk": "^1.0.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
