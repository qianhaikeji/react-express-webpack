{
  "name": "express-react-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --config webpack/webpack.config.js --inline --content-base client/ --history-api-fallback",
    "start:prod": "npm run build:prod && nodemon server.js",
    "clean": "rimraf public",
    "build:prod": "npm run clean && webpack --config webpack/webpack.config.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "if-env": "^1.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "http-server": "^0.8.5",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "html-webpack-plugin": "^2.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "css-loader": "^0.23.1",
    "nodemon": "^1.9.2"
  }
}
